{
  "name": "healthy-person-emulator-dotorg",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "prisma generate && remix vite:build",
    "dev": "remix vite:dev --host 0.0.0.0 --port 3000",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "generate:parquet": "tsx scripts/generateParquetFiles.ts",
    "start": "remix-serve ./build/server/index.js",
    "seed": "tsx prisma/seed.ts",
    "reset:db": "prisma migrate reset -f && prisma db push",
    "typecheck": "tsc",
    "knip": "knip",
    "test": "vitest"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "1.31.1-dev31.0",
    "@google-cloud/storage": "^7.17.3",
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^3.10.0",
    "@marsidev/react-turnstile": "^1.3.1",
    "@prisma/client": "^6.19.0",
    "@remix-run/node": "^2.17.2",
    "@remix-run/react": "^2.17.2",
    "@remix-run/route-config": "^2.17.2",
    "@remix-run/routes-option-adapter": "^2.17.2",
    "@remix-run/serve": "^2.17.2",
    "chalk": "^5.6.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "diff": "^7.0.0",
    "es-toolkit": "^1.42.0",
    "feed": "^5.1.0",
    "framer-motion": "^11.18.2",
    "happy-dom": "^16.8.1",
    "html-react-parser": "^5.2.10",
    "isbot": "^5.1.32",
    "jotai": "^2.15.2",
    "lucide-react": "^0.555.0",
    "marked": "~15.0.12",
    "node-html-markdown": "^1.3.0",
    "openai": "^4.104.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.67.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^9.1.0",
    "react-transition-group": "^4.4.5",
    "remark-gfm": "^4.0.1",
    "remix-auth": "3.7.0",
    "remix-auth-google": "2.0.0",
    "remix-flat-routes": "^0.8.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
